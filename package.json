    "i18next-browser-languagedetector": "^8.0.4",
    "lodash-es": "^4.17.23",
    "mobx": "^6.13.6",
    "mobx-react-lite": "^4.1.0",
    "react-error-boundary": "^6.0.3",
    "react-i18next": "^16.5.2"
  },
  "overrides": {
