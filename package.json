{
  "name": "@wandelbots/wandelbots-js-react-components",
  "version": "0.0.0-semantically-released",
  "description": "React UI toolkit for building applications on top of the Wandelbots platform",
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "storybook dev -p 6006 --no-open",
    "dev:pack": "nodemon -w \".\" -e '*' -i '*.tgz' -i dist -x \"npm run build && npm pack\"",
    "dev:wbjs": "nodemon --watch $WBJS_PATH -e tgz --exec 'npm install file:$WBJS_PATH && storybook dev -p 6006 --no-open'",
    "tsc": "tsc --pretty --noEmit",
    "test": "npm run build",
    "build": "rimraf dist && vite build && tsc --declaration --emitDeclarationOnly",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wandelbotsgmbh/wandelui.git"
  },
  "keywords": [
    "ui",
    "toolkit",
    "wandelbots"
  ],
  "author": "Wandelbots",
  "license": "Apache-2.0",
  "devDependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^5.13.6",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.2.7",
    "@storybook/addon-interactions": "^8.2.7",
    "@storybook/addon-links": "^8.2.7",
    "@storybook/blocks": "^8.2.7",
    "@storybook/react": "^8.2.7",
    "@storybook/react-vite": "^8.2.7",
    "@storybook/test": "^8.2.7",
    "@storybook/test-runner": "^0.19.1",
    "@svgr/rollup": "^8.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.40",
    "prettier-eslint": "^16.3.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.2",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-gltf": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "semantic-release": "^24.0.0",
    "storybook": "^8.2.7",
    "storybook-dark-mode": "^4.0.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": ">=5",
    "@react-spring/three": ">=9",
    "@react-three/drei": ">=9",
    "@react-three/fiber": ">=8",
    "@wandelbots/wandelbots-js": ">=1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": ">=0.167",
    "three-stdlib": ">=2"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^5.16.6",
    "@mui/lab": "^5.0.0-alpha.173",
    "@shikijs/monaco": "^1.12.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "lodash-es": "^4.17.21",
    "mobx": "^6.13.1",
    "mobx-react-lite": "^4.0.7",
    "react-i18next": "^15.0.0",
    "shiki": "^1.12.0"
  }
}
